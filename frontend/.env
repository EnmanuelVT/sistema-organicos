# Must be the SAME backend that validates the JWT it issues
VITE_API_BASE_URL=https://localhost:7232
# If you issue tokens from a different host/port (e.g., http://localhost:5062),
# set this (or just point VITE_API_BASE_URL there as well).
VITE_AUTH_BASE_URL=http://localhost:5062

# Ensure both files use the same storage keys
VITE_TOKEN_STORAGE_KEY=accessToken
VITE_REFRESH_TOKEN_STORAGE_KEY=refreshToken
